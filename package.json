{
  "name": "block-chain-wallet",
  "version": "1.0.4",
  "description": "block-chain wallet",
  "scripts": {
    "pack": "webpack --mode=production",
    "build": "rm -rf ./dist && tsc",
    "clean": "rm -rf ./dist ./bundle",
    "bundle": ". ./.env && npm run clean && node ./script.js bundle",
    "start": ". ./.env && node ./script.js start",
    "watch": "npm run build && node ./dist/server.js",
    "srv": ". ./.env && nodemon --watch server server/http-server.js",
    "p2p": ". ./.env && node server/p2p-server.js",
    "svd": ". ./.env && node server/svd-server.js",
    "gw": ". ./.env && node server/gw-server.js",
    "test": "jest --watchAll"
  },
  "author": "Aryan Alikhani",
  "license": "MIT",
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/express": "^4.17.2",
